alias ls="ls -lH"
cl () {
  cd $1
  ls 
}
alias ..="cd .."
alias mv="mv -iv"
alias cp="cp -iv"
alias lsa="ls -a"
alias vi="vim"
alias vimrc="vim ~/.vimrc"
alias als="vim ~/.bash_aliases"
alias t="tmux"
alias ta="tmux attach"
alias home="cl ~"
alias prev="cl -"
alias clr="clear"
alias brc="vim ~/.bashrc"
alias srcb="source ~/.bashrc"



